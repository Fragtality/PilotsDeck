<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <ApplicationIcon>PluginIcon.ico</ApplicationIcon>
    <PlatformTarget>x64</PlatformTarget>
    <Version>0.7.8.0</Version>
    <Authors>Fragtality</Authors>
    <Description>Directly control your FlightSim from your StreamDeck via FSUIPC!</Description>
    <Copyright>Copyright Â© 2023</Copyright>
    <PackageIcon>PluginIcon.png</PackageIcon>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
    <IncludeSymbols>False</IncludeSymbols>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>embedded</DebugType>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DebugType>embedded</DebugType>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Includes\**" />
    <EmbeddedResource Remove="Includes\**" />
    <None Remove="Includes\**" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="PluginIcon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Actions\" />
    <Folder Include="Images\category\" />
    <Folder Include="Images\korry\" />
    <Folder Include="Models\" />
    <Folder Include="Profiles\" />
    <Folder Include="PI\css\" />
    <Folder Include="PI\js\" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\Artwork\category\PluginIcon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="FSUIPCClientDLL" Version="3.3.3" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
    <PackageReference Include="StreamDeckLib" Version="2302.6.2128" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Microsoft.FlightSimulator.SimConnect">
      <HintPath>Microsoft.FlightSimulator.SimConnect.dll</HintPath>
	  <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
    </Reference>
    <Reference Include="vJoyInterfaceWrap">
      <HintPath>vJoyInterfaceWrap.dll</HintPath>
    </Reference>
  </ItemGroup>
  
  <ItemGroup>
    <PublishReadyToRunExclude Include="Microsoft.FlightSimulator.SimConnect.dll" />
    <PublishReadyToRunExclude Include="vJoyInterface.dll" />
	<PublishReadyToRunExclude Include="vJoyInterfaceWrap.dll" />
	<PublishReadyToRunExclude Include="FSUIPC_WAPID.dll" />
	<PublishSingleFileExclude Include="Microsoft.FlightSimulator.SimConnect.dll" />
  </ItemGroup>

